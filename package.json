    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.0",
